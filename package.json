{
  "name": "modernode",
  "version": "0.1.1",
  "description": "Modern Node.js boilerplate",
  "author": "Andrey Zharikov",
  "repository": {
    "type": "git",
    "url": "https://github.com/zharikovpro/modernode.git"
  },
  "license": "MIT",
  "main": "app.js",
  "engines": {
    "node": "8.7.0"
  },
  "scripts": {
    "lint": "eslint *.js test/ src/",
    "test": "jest",
    "dev": "nodemon ./src/app.js --exec babel-node",
    "compile": "rm -rf dist/* && babel ./src -d dist",
    "start": "node ./dist/app.js"
  },
  "dependencies": {
    "babel-cli": "^6.11.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.11.6",
    "dotenv": "^2.0.0"
  },
  "devDependencies": {
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.4.1",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-react": "^5.2.2",
    "jest": "^21.2.1",
    "nodemon": "^1.10.0"
  }
}
